{
 "name": "@toolbuilder/isnumber",
 "version": "0.1.1",
 "description": "Well tested 'is number' checks, that can optionally include number-like strings, and non-finite values.",
 "keywords": [
  "number",
  "numeric",
  "string",
  "finite"
 ],
 "homepage": "https://github.com/toolbuilder/isnumber#readme",
 "bugs": {
  "url": "https://github.com/toolbuilder/isnumber/issues"
 },
 "repository": {
  "type": "git",
  "url": "https://github.com/toolbuilder/isnumber.git"
 },
 "license": "MIT",
 "author": "Kevin Hudson",
 "main": "index.js",
 "unpkg": "umd/isnumber.min.js",
 "module": "src/isnumber.js",
 "directories": {
  "test": "test"
 },
 "files": [
  "src",
  "index.js",
  "umd"
 ],
 "scripts": {
  "build": "npm-run-all --serial test test:packfile lint lint:packagejson bundle docs",
  "bundle": "npm-run-all --serial clean bundle:rollup",
  "bundle:rollup": "rollup -c rollup.config.js",
  "clean": "rimraf ./umd",
  "docs": "node -r esm ./scripts/docgen.js",
  "lint": "eslint src test",
  "lint:packagejson": "npmPkgJsonLint .",
  "prepublishOnly": "npm run build",
  "prerelease": "git status --porcelain | grep \"M|A\"",
  "release": "standard-version",
  "test": "tape -r esm test/*_test.js | tap-nirvana",
  "test:packfile": "rollup --config node:@toolbuilder/rollup-config-pkgtest"
 },
 "eslintConfig": {
  "extends": "@toolbuilder/eslint-config"
 },
 "dependencies": {
  "esm": "^3.2.25"
 },
 "devDependencies": {
  "@toolbuilder/eslint-config": ">=0.1.3",
  "@toolbuilder/package-json-lint-config": "^0.1.0",
  "@toolbuilder/rollup-config-pkgtest": "^0.1.0",
  "documentation": "^13.0.1",
  "eslint": "^7.2.0",
  "markedpp": "^1.0.2",
  "npm-package-json-lint": "^5.1.0",
  "npm-run-all": "^4.1.5",
  "rimraf": "^3.0.2",
  "rollup": "^2.17.0",
  "rollup-plugin-terser": "^6.1.0",
  "standard-version": "^8.0.0",
  "tap-nirvana": "^1.1.0",
  "tape": "^5.0.1"
 },
 "npmpackagejsonlint": {
  "extends": "@toolbuilder/package-json-lint-config"
 }
}
